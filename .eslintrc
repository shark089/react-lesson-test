{
  "extends": "airbnb-base",
  "plugins": [
    "html",
    "react"
  ],
  "env": {
    "browser": true
  },
  "globals": {
    "React": true,
    "ReactDOM": true,
    "axios": true,
    "FB": true
  },
  "parser": "babel-eslint",
  "rules": {
    "func-names": 0,
    "jsx-quotes": 1,
    "no-console": 0,
    "no-unused-vars": 0,
    "no-var": 0,
    "object-shorthand": 0,
    "prefer-arrow-callback": 0,
    "prefer-destructuring": 0,
    "space-in-parens": 1,
    "vars-on-top": 0,
    "comma-dangle": [
      1,
      {
        "arrays": "always-multiline",
        "objects": "always-multiline",
        "imports": "always-multiline",
        "exports": "always-multiline",
        "functions": "never"
      }
    ]
  }
}
